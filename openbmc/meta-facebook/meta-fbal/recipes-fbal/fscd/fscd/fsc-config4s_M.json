{
    "version": "fbal-r06",
    "pwm_transition_value": 70,
    "pwm_boost_value": 100,
    "sample_interval_ms": 3000,
    "fan_recovery_time": 2,
    "boost": {
      "fan_fail": true,
      "sensor_fail": false
    },
    "sensor_valid_check": {
      "mb": {
        "attribute": {
          "type" : "power_status"
        }
      }
    },  
    "watchdog": false,
    "min_rpm": 500,
    "profiles": {
      "linear_dimm": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "linear",
        "data": [
          [60, 10],
          [65, 15],
          [70, 20],
          [74, 25]
        ]
      },
      "pid_dimm": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "pid",
        "positive_hysteresis": 1,
        "negative_hysteresis": 2,      
        "setpoint": 78,
        "kp": -2,
        "ki": -0.035,
        "kd": 0
      },
      "linear_mezz": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "linear",
        "data": [
          [65, 10],
          [70, 15],
          [72, 20],
          [75, 25],
          [78, 30],
          [80, 35],
          [83, 40],
          [85, 50],
          [90, 65],
          [95, 75]
        ]
      },
      "linear_inlet": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "linear",
        "data": [
          [25, 30],
          [30, 40],
          [35, 50],
          [38, 60],
          [40, 80]
        ]
      },
      "linear_vr": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "linear",
        "data": [
          [70, 15],
          [75, 20],
          [80, 25],
          [85, 35],
          [90, 60],
          [95, 65]
        ]
      },
      "linear_cpu_margin": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "linear",
        "data": [
          [-20, 10],
          [-18, 15],
          [-15, 25]
        ]
      },
      "pid_cpu_margin": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "pid",
        "positive_hysteresis": 1,
        "negative_hysteresis": 2,     
        "setpoint": -10,
        "kp": -1.25,
        "ki": -0.035,
        "kd": 0
      }
    },
    "fans": {
      "0": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 0"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 0"
        }
      },
      "1": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 1"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 0"
        }
      },
      "2": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 2"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 1"
        }
      },
      "3": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 3"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 1"
        }
      },
      "4": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 4"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 2"
        }
      },
      "5": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 5"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 2"
        }
      },
      "6": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 6"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 3"
        }
      },
      "7": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 7"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 3"
        }
      }
    },
     "zones": {
      "zone_1": {
        "pwm_output": [0, 2, 4, 6],
        "expr_file": "zone1_4s_M.fsc"
      }
    }
}
